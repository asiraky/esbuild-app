{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": "./src",
    "paths": {
      "~/*": [
        "./*"
      ]
    },
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "jsxImportSource": "@emotion/react"
  },
  "include": [
    "next-env.d.ts",
    "./src"
  ],
  "exclude": [
    "node_modules",
  ]
}
